{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx nest start --watch",
    "build": "npx nest build",
    "start": "node dist/main",
    "lint": "npx eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npx jest --passWithNoTests",
    "test:watch": "npx jest --watch --passWithNoTests",
    "test:cov": "npx jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --passWithNoTests",
    "test:e2e": "npx jest --config ./test/jest-e2e.json --passWithNoTests",
    "typecheck": "npx tsc --noEmit",
    "type-check": "npx tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.957.0",
    "@aws-sdk/s3-request-presigner": "^3.957.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/throttler": "^6.5.0",
    "@packages/database": "workspace:*",
    "@packages/shared": "workspace:*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "handlebars": "^4.7.8",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "stripe": "^20.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@packages/config": "workspace:*",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}